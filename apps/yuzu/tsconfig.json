{
	"compilerOptions": {
		"target": "ES2021",
		"skipLibCheck": true,
		"strict": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"types": ["bun-types"],
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"@momoi/*": ["./src/*"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": ["node_modules"]
}
